#set("title") {
    Follower of
    #for(n in screenNames) {
        #(n) #if(isFirst) { &amp; }
    }
}

<html>
    <head>
        <title>#get(title)</title>
        <h1>
            #get(title)
        </h1>
    </head>
    <body>
        <ul>
            #for(u in users) {
                <li>@#(u.screenName) (#(u.id)) #(u.name)</li>
            }
        </ul>
    </body>
</html>
