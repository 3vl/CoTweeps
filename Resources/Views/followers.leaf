<html>
    <head>
        <title>Followers of #{screenName}</title>
    </head>
    <body>
        <h1>#(screenName)</h1>
        <ul>
            #for(u in users) {
                <li>@#(u.screenName) (#(u.id)) #(u.name)</li>
            }
        </ul>
        <p>
            #(userCursor.nextCursor)
        </p>
    </body>
</html>
